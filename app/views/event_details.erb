<h1><%= @event_name %> Statistics</h1>

<h2>Hour By Hour Breakdown</h2>
<table id="hour-by-hour">
  <tr>
    <th>Hour</th>
    <th>Times Received</th>
  </tr>
  <% @event_hourly_breakdown.each do |hour, times_received| %>
  <tr>
    <td><%= hour %></td>
    <td><%= times_received %></td>
  </tr>
  <% end %>
</table>
